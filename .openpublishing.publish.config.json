{
  "docsets_to_publish": [
    {
      "docset_name": "e2eppe-core-docs",
      "build_source_folder": ".",
      "build_output_subfolder": "e2eppe-core-docs",
      "locale": "zh-cn",
      "monikers": [],
      "xref_query_tags": [
        "/dotnet",
        "/uwp/api"
      ],
      "filemap_share_depots": [
        "MSDN.e2e-ppe-dotnetapidocs"
      ],
      "open_to_public_contributors": false,
      "type_mapping": {
        "ZonePivotGroups": "Toc",
        "Conceptual": "Content",
        "ManagedReference": "Content",
        "RestApi": "Content",
        "Tutorial": "Content"
      },
      "build_entry_point": "docs",
      "template_folder": "_themes"
    }
  ],
  "notification_subscribers": [],
  "branches_to_filter": [
    "live-sxs"
  ],
  "git_repository_url_open_to_public_contributors": "https://github.com/dotnet/docs.zh-cn",
  "git_repository_branch_open_to_public_contributors": "live",
  "continue_with_document_error": true,
  "skip_source_output_uploading": false,
  "need_preview_pull_request": true,
  "need_pr_comments": false,
  "contribution_branch_mappings": {},
  "dependent_repositories": [
    {
      "path_to_root": "_themes",
      "url": "https://github.com/Microsoft/templates.docs.msft.zh-cn",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_themes.pdf",
      "url": "https://github.com/Microsoft/templates.docs.msft.pdf",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_csharplang",
      "url": "https://github.com/dotnet/csharplang",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_vblang",
      "url": "https://github.com/dotnet/vblang",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "machinelearning-samples",
      "url": "https://github.com/dotnet/machinelearning-samples",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "dotnet-samples",
      "url": "https://github.com/dotnet/samples",
      "branch": "master",
      "branch_mapping": {}
    },
    {
      "path_to_root": "_repo.en-us",
      "url": "https://github.com/OPS-E2E-PPE/docs",
      "branch": "live",
      "branch_mapping": {
        "live": "live",
        "master": "master"
      }
    }
  ],
  "branch_target_mapping": {
    "live": [
      "Publish",
      "Pdf"
    ],
    "master": [
      "Publish",
      "Pdf"
    ]
  },
  "need_generate_pdf_url_template": true,
  "targets": {
    "Pdf": {
      "template_folder": "_themes.pdf"
    }
  },
  "docs_build_engine": {
    "name": "docfx_v3"
  },
  "need_generate_pdf": false,
  "need_generate_intellisense": false,
  "xref_endpoint": "https://xref.docs.microsoft.com",
  "dependent_packages": [
    {
      "id": "memberpage.plugins",
      "nuget_feed": "https://www.myget.org/F/docfx/api/v2",
      "path_to_root": "_dependentPackages/memberpage.plugins",
      "target_framework": "net45",
      "version": "latest"
    }
  ]
}